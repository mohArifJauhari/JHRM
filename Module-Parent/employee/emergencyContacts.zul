<zk>
	<window id="windowEmergencyContact" border="none">
		<borderlayout height="500px">
			<!-- Function CRUD -->
			<!-- <north/> -->
			<!-- Table Database -->
			<north>
				<vbox>
					<groupbox mold="3d">
						<caption label="Assigned Emergency Contacts"></caption>
						<hbox>
							<button label="${labels.button.add}"/>
							<button label="${labels.button.delete}" />
						</hbox>
						<space />
						<listbox>
							<listhead>
								<listheader width="5%" align="center" />
								<listheader label="Name" sort="auto" />
								<listheader label="Relationship"
									sort="auto" />
								<listheader label="Home Telephone"
									sort="auto" />
								<listheader label="Mobile" sort="auto" />
								<listheader label="Work Telephone]"
									sort="auto" />
							</listhead>
							<listitem>
								<listcell
									image="/images/icons/btn_edit.gif" tooltiptext="Click For Edit"/>
								<listcell label="toke" />
								<listcell label="anak" />
								<listcell label="123" />
								<listcell label="123" />
								<listcell label="123" />
							</listitem>
						</listbox>
					</groupbox>
				</vbox>
			</north>
			<center>
				<vbox>
					<groupbox mold="3d">
						<caption label="Attachments" />
						<grid>
							<columns>
								<column label="" />
							</columns>
							<rows>
								<row>
									<button label="Add" />
								</row>
							</rows>
						</grid>
					</groupbox>
				</vbox>
			</center>
			<south />
		</borderlayout>
	</window>
</zk>